{
  "name": "web",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config --require esbuild-register ./server.js --watch ./server.js",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node --require esbuild-register ./server.js",
    "typecheck": "tsc",
    "lint": "eslint .",
    "playwright:install": "playwright install --with-deps",
    "test:e2e": "playwright test",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static -p 6007"
  },
  "dependencies": {
    "@kanban/clock": "workspace:*",
    "@kanban/database": "workspace:*",
    "@radix-ui/react-dialog": "1.0.3",
    "@radix-ui/react-navigation-menu": "1.1.2",
    "@remix-run/css-bundle": "1.15.0",
    "@remix-run/express": "1.15.0",
    "@remix-run/node": "1.15.0",
    "@remix-run/react": "1.15.0",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "esbuild-register": "3.4.2",
    "express": "4.18.2",
    "framer-motion": "10.12.3",
    "isbot": "3.6.8",
    "morgan": "1.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.4",
    "@kanban/eslint-config-bases": "workspace:*",
    "@kanban/tsconfig": "workspace:*",
    "@playwright/test": "1.31.2",
    "@remix-run/dev": "1.15.0",
    "@remix-run/eslint-config": "1.15.0",
    "@remix-run/server-runtime": "1.15.0",
    "@remix-run/testing": "1.15.0",
    "@storybook/addon-essentials": "7.0.5",
    "@storybook/addon-interactions": "7.0.5",
    "@storybook/addon-links": "7.0.5",
    "@storybook/blocks": "7.0.5",
    "@storybook/jest": "0.1.0",
    "@storybook/react": "7.0.5",
    "@storybook/react-vite": "7.0.5",
    "@storybook/react-webpack5": "7.0.5",
    "@storybook/testing-library": "0.1.0",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@vanilla-extract/css": "1.11.0",
    "@vanilla-extract/vite-plugin": "3.8.0",
    "@vitejs/plugin-react": "3.1.0",
    "dotenv": "16.0.3",
    "eslint": "8.38.0",
    "http-server": "14.1.1",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "prop-types": "15.8.1",
    "storybook": "7.0.5",
    "typescript": "4.9.5",
    "vite": "4.2.1"
  },
  "engines": {
    "node": ">=14"
  }
}
