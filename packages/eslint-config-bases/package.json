{
  "name": "@kanban/eslint-config-bases",
  "version": "0.0.0",
  "type": "commonjs",
  "main": "./src/index.js",
  "exports": {
    ".": {
      "require": "./src/index.js"
    },
    "./patch/modern-module-resolution": {
      "require": "./src/patch/modern-module-resolution.js"
    },
    "./prettier": {
      "require": "./src/bases/prettier.js"
    },
    "./react": {
      "require": "./src/bases/react.js"
    },
    "./regex": {
      "require": "./src/bases/regex.js"
    },
    "./rtl": {
      "require": "./src/bases/rtl.js"
    },
    "./sonar": {
      "require": "./src/bases/sonar.js"
    },
    "./storybook": {
      "require": "./src/bases/storybook.js"
    },
    "./typescript": {
      "require": "./src/bases/typescript.js"
    }
  },
  "scripts": {
    "lint": "eslint  --ext .ts,.js,.cjs,.mjs --cache --cache-location ../../.cache/eslint/eslint-config-bases.eslintcache"
  },
  "devDependencies": {
    "@kanban/tsconfig": "workspace:*",
    "@types/node": "18.15.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "eslint": "8.36.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "eslint": "^8.34.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint-config-prettier": "8.7.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-regexp": "1.12.0",
    "eslint-plugin-sonarjs": "0.18.0",
    "eslint-plugin-storybook": "0.6.11",
    "eslint-plugin-testing-library": "5.10.2",
    "prettier": "2.8.4"
  }
}
